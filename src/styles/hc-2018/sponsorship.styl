#sponsorship-hack-cambridge-logo-object {
    height: var(--two);
}
#sponsorship-hack-cambridge-wordmark {
    font-size: 21.3953315789pt;
    margin-bottom: 20pt;
}
#sponsorship-university-of-cambridge-logo-object {
    height: var(--half);
    width: initial;
    flex-basis: var(--half);
}
#sponsorship-intro-text {
    max-width: 311pt;
    margin-left: 0;
}
#sponsorship-university-of-cambridge-text {
    flex: auto;
    margin: 0;
    min-width: 193.359375px;
}
#sponsorship-university-of-cambridge-endorsement {
    flex-direction: row;
    flex-wrap: wrap;
}

@media (max-width: 595pt) {
    #sponsorship-pitch-image {
        height: 215pt;
    }
    .unit.h:not(:first-child) {
        margin-top: 28.5pt;
    }
    
    @media (min-width: 300px) {
        #sponsorship-university-of-cambridge-logo-object {
            margin-right: 24pt;
        }
    }
}

@media (max-width: 502px) {
    #sponsorship-university-of-cambridge-logo-object {
        margin-bottom: 24pt;
    }
}

@media (max-width: 414px) {
    #sponsorship-hack-cambridge-logo-object {
        width: 34.8845108696vw;
        height: 30.2121074879vw;
    }
    #sponsorship-hack-cambridge-wordmark {
        font-size: 6.8906060739vw;
    }
}