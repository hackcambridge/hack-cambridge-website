{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "interface-name": [true, "never-prefix"],
        "no-console": false,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "trailing-comma": [true, {
            "objects": "never",
            "arrays": "never",
            "functions": "never",
            "typeLiterals": "never"
        }]
    },
    "rulesDirectory": []
}