{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "max-line-length": [true, 150],
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [false]
  },
  "rulesDirectory": []
}